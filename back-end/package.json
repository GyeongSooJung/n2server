    "@nestjs/platform-express": "^8.1.2",
    "@nestjs/swagger": "^5.1.4",
    "@typegoose/typegoose": "^9.2.0",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "generate-random-numbers": "^1.0.3",
